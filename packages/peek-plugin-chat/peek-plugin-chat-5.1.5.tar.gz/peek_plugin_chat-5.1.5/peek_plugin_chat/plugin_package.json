{
    "plugin": {
        "title": "Chat",
        "packageName": "peek_plugin_chat",
        "version": "5.1.5",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "logic",
        "field",
        "storage",
        "admin",
        "admin-doc",
        "field-doc",
        "office-doc"
    ],
    "admin-doc": {
        "docDir": "admin-doc",
        "docRst": "index.rst",
        "hasApi": true
    },
    "field-doc": {
        "docDir": "both-doc",
        "docRst": "index.rst"
    },
    "office-doc": {
        "docDir": "both-doc",
        "docRst": "index.rst"
    },
    "logic": {},
    "storage": {
        "alembicDir": "_private/alembic"
    },
    "admin": {
        "showHomeLink": true,
        "appDir": "_private/admin-app",
        "appModule": "chat.module#ChatModule",
        "moduleDir": "plugin-module"
    },
    "field": {
        "showHomeLink": true,
        "appDir": "_private/mobile-app",
        "appModule": "chat.module#ChatModule",
        "assetDir": "_private/mobile-assets",
        "icon": "/assets/peek_plugin_chat/icon.png",
        "moduleDir": "plugin-module"
    },
    "office": {
        "moduleDir": "plugin-module"
    }
