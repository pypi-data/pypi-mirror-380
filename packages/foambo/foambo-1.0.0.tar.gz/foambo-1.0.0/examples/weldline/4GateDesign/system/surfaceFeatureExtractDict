FoamFile
{
version 2.0;
format ascii;
class dictionary;
object surfaceFeatureExtractDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //


walls.stl
{
extractionMethod extractFromSurface;
includedAngle 150;
curvature true;
geometricTestOnly yes;
intersectionMethod none;
writeObj no;
writeVTK yes;
}

inlet.stl
{
extractionMethod extractFromSurface;
includedAngle 150;
curvature true;
geometricTestOnly yes;
intersectionMethod none;
writeObj no;
writeVTK yes;
}

contact.stl
{
extractionMethod extractFromSurface;
includedAngle 150;
curvature true;
geometricTestOnly yes;
intersectionMethod none;
writeObj no;
writeVTK yes;
}


