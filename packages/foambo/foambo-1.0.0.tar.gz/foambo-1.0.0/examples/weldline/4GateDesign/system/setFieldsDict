// Created: 2025-08-12 15:31:50

FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      setFieldsDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    volScalarFieldValue alpha1 0
);

regions
(
    boxToCell
    {
        box (-0.005 -0.005 0.07) (0.005 0.005 0.12);
        fieldValues
        (
            volScalarFieldValue alpha1 1
            volScalarFieldValue T 518.15
        );
    }
);

