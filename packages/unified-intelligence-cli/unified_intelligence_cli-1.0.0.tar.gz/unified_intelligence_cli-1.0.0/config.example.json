{
  "provider": "grok",
  "provider_config": {
    "model": "grok-code-fast-1",
    "temperature": 0.7,
    "max_tokens": 500
  },
  "parallel": true,
  "timeout": 120,
  "verbose": true,
  "custom_agents": [
    {
      "role": "security_analyst",
      "capabilities": ["security", "audit", "vulnerability", "pentest"]
    },
    {
      "role": "devops",
      "capabilities": ["deploy", "ci/cd", "docker", "kubernetes", "infrastructure"]
    }
  ]
}