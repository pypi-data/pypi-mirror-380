# QuantRS2 Environment Variables Template
# Copy this file to .env and fill in the appropriate values

# Environment
QUANTRS2_ENVIRONMENT=development

# Security (REQUIRED for production)
QUANTRS2_SECRET_KEY=your-secret-key-here
QUANTRS2_JWT_SECRET=your-jwt-secret-here
QUANTRS2_ENCRYPTION_KEY=your-encryption-key-here

# Database Configuration
QUANTRS2_DB_HOST=localhost
QUANTRS2_DB_PORT=5432
QUANTRS2_DB_NAME=quantrs2
QUANTRS2_DB_USER=quantrs2_user
QUANTRS2_DB_PASSWORD=your-db-password-here

# Quantum Backend API Tokens
# IBM Quantum
QUANTRS2_IBM_API_TOKEN=your-ibm-token-here
QUANTRS2_IBM_PROJECT_ID=your-ibm-project-id

# Google Quantum AI
QUANTRS2_GOOGLE_API_TOKEN=your-google-token-here
QUANTRS2_GOOGLE_PROJECT_ID=your-google-project-id

# AWS Braket
QUANTRS2_AWS_ACCESS_KEY=your-aws-access-key
QUANTRS2_AWS_SECRET_KEY=your-aws-secret-key
QUANTRS2_AWS_REGION=us-east-1

# Performance Settings
QUANTRS2_MAX_QUBITS=30
QUANTRS2_MEMORY_LIMIT=8192
QUANTRS2_MAX_JOBS=4

# Logging
QUANTRS2_LOG_LEVEL=INFO
QUANTRS2_LOG_FILE=/var/log/quantrs2.log

# Monitoring
QUANTRS2_METRICS_PORT=9090
QUANTRS2_HEALTH_PORT=8080
QUANTRS2_ALERT_WEBHOOK=https://hooks.slack.com/your-webhook-url

# Optional: Custom Settings
QUANTRS2_ENABLE_GPU=false
QUANTRS2_ENABLE_ANALYTICS=true
QUANTRS2_DEBUG=false