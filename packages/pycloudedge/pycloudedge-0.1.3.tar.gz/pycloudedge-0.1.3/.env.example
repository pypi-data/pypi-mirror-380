# CloudEdge API Configuration
# Copy this file to .env and fill in your credentials

# CloudEdge Account Credentials
CLOUDEDGE_USERNAME=your-email@example.com
CLOUDEDGE_PASSWORD=your-password

# Location Settings
CLOUDEDGE_COUNTRY_CODE=IT
CLOUDEDGE_PHONE_CODE=+39

# Optional: Debug mode (true/false)
CLOUDEDGE_DEBUG=false