<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">

<xs:attribute name="name" type="xs:string" />

<xs:simpleType name="accessRule">
  <xs:restriction base="xs:string">
    <xs:enumeration value="allow" />
    <xs:enumeration value="deny" />
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="dimensionType">
  <xs:restriction base="xs:string">
    <xs:enumeration value="geo" />
    <xs:enumeration value="time" />
    <xs:enumeration value="standard" />
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="columnType">
  <xs:restriction base="xs:string">
    <xs:enumeration value="bool" />
    <xs:enumeration value="date" />
    <xs:enumeration value="time" />
    <xs:enumeration value="dttm" />
    <xs:enumeration value="stmp" />
    <xs:enumeration value="f32" />
    <xs:enumeration value="f64" />
    <xs:enumeration value="i8" />
    <xs:enumeration value="i16" />
    <xs:enumeration value="i32" />
    <xs:enumeration value="i64" />
    <xs:enumeration value="i128" />
    <xs:enumeration value="u8" />
    <xs:enumeration value="u16" />
    <xs:enumeration value="u32" />
    <xs:enumeration value="u64" />
    <xs:enumeration value="u128" />
    <xs:enumeration value="str" />
  </xs:restriction>
</xs:simpleType>

<xs:simpleType name="inlineTableFormat">
  <xs:restriction base="xs:string">
    <xs:enumeration value="tuples" />
    <xs:enumeration value="csv" />
  </xs:restriction>
</xs:simpleType>

<xs:complexType name="AccessElement">
  <xs:attribute name="rule" type="accessRule" use="required" />
  <xs:attribute name="roles" type="xs:string" use="required" />
</xs:complexType>

<xs:complexType name="AnnotationElement">
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute ref="name" use="required" />
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<xs:complexType name="LocalizedAttrElement">
  <xs:simpleContent>
    <xs:extension base="xs:string">
      <xs:attribute name="attr" type="xs:string" use="required" />
      <xs:attribute name="locale" type="xs:string" use="required" />
      <xs:attribute name="value" type="xs:string" />
    </xs:extension>
  </xs:simpleContent>
</xs:complexType>

<xs:complexType name="CubeElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Access" type="AccessElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
    <xs:element ref="TableKind" />
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="DimensionUsage" type="DimensionUsageElement" minOccurs="0" />
    <xs:element name="Dimension" type="DimensionElement" minOccurs="0" />
    <xs:element name="Measure" type="MeasureElement" minOccurs="0" />
    <xs:element name="CalculatedMeasure" type="CalculatedMeasureElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="public" type="xs:boolean" />
  <xs:attribute name="subset_table" type="xs:boolean" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="TableElement">
  <xs:attribute ref="name" use="required" />
  <xs:attribute name="primary_key" type="xs:string" />
</xs:complexType>

<xs:complexType name="InlineTableElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Row" type="InlineTableRowElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute name="name" type="xs:string" use="required" />
  <xs:attribute name="format" type="inlineTableFormat" />
</xs:complexType>

<xs:complexType name="InlineTableRowElement">
  <xs:simpleContent>
    <xs:extension base="xs:string" />
  </xs:simpleContent>
</xs:complexType>

<xs:complexType name="TableUsageElement">
  <xs:attribute name="source" type="xs:string" />
</xs:complexType>

<xs:complexType name="DimensionUsageElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
    <xs:element name="HierarchyUsage" type="HierarchyUsageElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="fkey_time" type="xs:string" />
  <xs:attribute name="foreign_key" type="xs:string" use="required" />
  <xs:attribute name="source" type="xs:string" use="required" />
  <xs:attribute name="type" type="dimensionType" default="standard" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="HierarchyUsageElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
    <xs:element name="LevelUsage" type="LevelUsageElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="source" type="xs:string" use="required" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="LevelUsageElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
    <xs:element name="PropertyUsage" type="PropertyUsageElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="source" type="xs:string" use="required" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="PropertyUsageElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="source" type="xs:string" use="required" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="DimensionElement">
  <xs:sequence>
    <xs:choice maxOccurs="unbounded">
      <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
      <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
    </xs:choice>
    <xs:element name="Hierarchy" type="HierarchyElement" minOccurs="1" maxOccurs="unbounded" />
  </xs:sequence>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="default_hierarchy" type="xs:string" />
  <xs:attribute name="foreign_key" type="xs:string" />
  <xs:attribute name="fkey_time" type="xs:string" />
  <xs:attribute name="type" type="dimensionType" default="standard" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="HierarchyElement">
  <xs:sequence>
    <xs:choice maxOccurs="unbounded">
      <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
      <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
      <xs:element ref="TableKind" minOccurs="0" />
    </xs:choice>
    <xs:element name="Level" type="LevelElement" minOccurs="1" maxOccurs="unbounded" />
  </xs:sequence>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="default_member" type="xs:string" />
  <xs:attribute name="primary_key" type="xs:string" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="LevelElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
    <xs:element name="Property" type="PropertyElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="key_column" type="xs:string" use="required" />
  <xs:attribute name="key_type" type="columnType" />
  <xs:attribute name="name_column" type="xs:string" />
  <xs:attribute name="time_scale" type="xs:string" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="PropertyElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="key_column" type="xs:string" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="MeasureElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="aggregator" type="xs:string" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="key_column" type="xs:string" use="required" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:complexType name="CalculatedMeasureElement">
  <xs:choice maxOccurs="unbounded">
    <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
    <xs:element name="LocalizedAttr" type="LocalizedAttrElement" minOccurs="0" />
  </xs:choice>

  <xs:attribute ref="name" use="required" />
  <xs:attribute name="caption" type="xs:string" />
  <xs:attribute name="formula" type="xs:string" />
  <xs:attribute name="visible" type="xs:boolean" />
</xs:complexType>

<xs:element name="TableKind" />
<xs:element substitutionGroup="TableKind" name="Table" type="TableElement" />
<xs:element substitutionGroup="TableKind" name="InlineTable" type="InlineTableElement" />
<xs:element substitutionGroup="TableKind" name="TableUsage" type="TableUsageElement" />

<xs:element name="Schema">
  <xs:complexType>
    <xs:choice maxOccurs="unbounded">
      <xs:element name="Annotation" type="AnnotationElement" minOccurs="0" />
      <xs:element name="InlineTable" type="InlineTableElement" minOccurs="0" />
      <xs:element name="SharedDimension" type="DimensionElement" minOccurs="0" />
      <xs:element name="Cube" type="CubeElement" minOccurs="0" />
    </xs:choice>

    <xs:attribute ref="name" use="required" />
    <xs:attribute name="default_locale" type="xs:string" />
  </xs:complexType>
</xs:element>

</xs:schema>
