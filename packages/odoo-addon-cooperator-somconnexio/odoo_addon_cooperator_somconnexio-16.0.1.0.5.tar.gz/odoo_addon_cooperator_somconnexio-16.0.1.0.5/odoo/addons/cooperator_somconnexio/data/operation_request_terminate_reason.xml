<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>
    <record id="operation_reason_expectations_not_met" model="operation.request.terminate.reason">
      <field name="name">El servei / producte no s’ajusta a les meves necessitats</field>
      <field name="code">expectations_not_met</field>
    </record>
    <record id="operation_reason_no_coverage" model="operation.request.terminate.reason">
      <field name="name">No tinc cobertura d'internet al meu domicili</field>
      <field name="code">no_coverage</field>
    </record>
    <record id="operation_reason_high_price" model="operation.request.terminate.reason">
      <field name="name">Preu massa elevat</field>
      <field name="code">high_price</field>
    </record>
    <record id="operation_reason_service_issues" model="operation.request.terminate.reason">
      <field name="name">Problemes en el servei (incidències, mal funcionament…)</field>
      <field name="code">service_issues</field>
    </record>
    <record id="operation_reason_customer_service_issues" model="operation.request.terminate.reason">
      <field name="name">Problemes amb l’atenció al client (manca d’agilitat, excessiu temps de resposta...)</field>
      <field name="code">customer_service_issues</field>
    </record>
    <record id="operation_reason_no_coop_consistency" model="operation.request.terminate.reason">
      <field name="name">Manca de coherència entre els valors de la cooperativa i l’execució</field>
      <field name="code">no_coop_consistency</field>
    </record>
    <record id="operation_reason_change_similar_initiative" model="operation.request.terminate.reason">
      <field name="name">Canvi a iniciativa similar i/o del meu territori</field>
      <field name="code">change_similar_initiative</field>
    </record>
    <record id="operation_reason_other" model="operation.request.terminate.reason">
      <field name="name">Altres</field>
      <field name="code">other</field>
    </record>
    <record id="operation_reason_ns_nc" model="operation.request.terminate.reason">
      <field name="name">Ns/Nc</field>
      <field name="code">ns_nc</field>
    </record>
  </data>
</odoo>
