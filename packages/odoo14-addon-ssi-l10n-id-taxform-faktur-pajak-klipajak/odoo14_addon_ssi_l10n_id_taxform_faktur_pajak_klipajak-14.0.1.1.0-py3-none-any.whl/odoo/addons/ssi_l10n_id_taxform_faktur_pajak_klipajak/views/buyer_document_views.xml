<odoo>
<record id="buyer_document_view_form" model="ir.ui.view">
    <field name="name">buyer_document - Klik Pajak</field>
    <field name="model">buyer_document</field>
    <field
            name="inherit_id"
            ref="ssi_l10n_id_taxform_faktur_pajak.buyer_document_view_form"
        />
    <field name="arch" type="xml">
        <data>
            <xpath expr="//field[@name='code']" position="after">
                <field name="klikpajak_code" />
            </xpath>
        </data>
    </field>
</record>
</odoo>
