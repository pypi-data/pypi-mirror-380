<odoo>
<data>

<!-- Faktur Pajak Keluaran: change_backend_ok -->
<record
            id="policy_template_faktur_pajak_keluaran_change_backend"
            model="policy.template_detail"
        >
    <field
                name="template_id"
                ref="ssi_l10n_id_taxform_faktur_pajak.policy_template_faktur_pajak_keluaran"
            />
    <field
                name="field_id"
                search="[('model_id.model','=','faktur_pajak_keluaran'),('name','=','change_backend_ok')]"
            />
    <field name="restrict_state" eval="0" />
    <field name="restrict_user" eval="1" />
    <field name="computation_method">use_group</field>
    <field name="group_ids" eval="[(6,0,[ref('base.group_system')])]" />
    <field name="restrict_additional" eval="0" />
</record>

</data>
</odoo>
