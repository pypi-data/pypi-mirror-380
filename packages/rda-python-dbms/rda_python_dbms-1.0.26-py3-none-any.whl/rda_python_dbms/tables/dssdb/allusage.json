{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "10/27/2005",
    "description": "table of all data usage information for a whole year"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "allusage",
  "pkey": "aidx",
  "column": [
    {
      "name": "aidx",
      "type": "serial",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "auto generated allusage index, internal use"
    },
    {
      "name": "email",
      "type": "varchar(80)",
      "comment": "user email address"
    },
    {
      "name": "org_type",
      "type": "varchar(5)",
      "comment": "organization type like DSS, UCAR or UNIV"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "user country name"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid"
    },
    {
      "name": "date",
      "type": "date",
      "comment": "date data accessed"
    },
    {
      "name": "time",
      "type": "time",
      "default": "'00:00:00'",
      "comment": "time data accessed"
    },
    {
      "name": "quarter",
      "type": "smallint",
      "comment": "quarter of the year data accessed"
    },
    {
      "name": "size",
      "type": "bigint",
      "comment": "bytes of data accessed"
    },
    {
      "name": "method",
      "type": "varchar(20)",
      "comment": "delv. methods: MSS,Web,Ftp,Tape,Cd,Disk,Paper,cArt,Micro"
    },
    {
      "name": "source",
      "type": "char(1)",
      "comment": "W - wusage, M - mssusage, O - ordusage"
    },
    {
      "name": "midx",
      "type": "smallint",
      "default": "0",
      "comment": "refer to mbr2loc.midx if not 0"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "IP address"
    },
    {
      "name": "region",
      "type": "varchar(80)",
      "comment": "user region name; for example, Colorado"
    }
  ],
  "index": [
    {
      "name": "allusage_idx_1",
      "column": "quarter"
    },
    {
      "name": "allusage_idx_2",
      "column": "dsid"
    },
    {
      "name": "allusage_idx_3",
      "column": "date"
    },
    {
      "name": "allusage_idx_4",
      "column": "email"
    },
    {
      "name": "allusage_idx_5",
      "column": "method"
    },
    {
      "name": "allusage_idx_6",
      "column": "country"
    }
  ],
  "comment": "ALL DATA FILE USAGE"
}