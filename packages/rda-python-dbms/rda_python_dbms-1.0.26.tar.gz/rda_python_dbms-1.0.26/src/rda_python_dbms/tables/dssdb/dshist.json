{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "11/14/2008",
    "description": "table of historical file information for each dataset"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "dshist",
  "pkey": "dsid, date",
  "column": [
    {
      "name": "dsid",
      "type": "varchar(9)",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "dataset id number"
    },
    {
      "name": "date",
      "type": "date",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "date file info recorded"
    },
    {
      "name": "pmsssize",
      "type": "bigint",
      "default": "0",
      "comment": "bytes of primary mss data files"
    },
    {
      "name": "msssize",
      "type": "bigint",
      "default": "0",
      "comment": "bytes of all mss data files"
    },
    {
      "name": "websize",
      "type": "bigint",
      "default": "0",
      "comment": "bytes of all web data files"
    },
    {
      "name": "pmsscnt",
      "type": "int",
      "default": "0",
      "comment": "primary MSS file count"
    },
    {
      "name": "msscnt",
      "type": "int",
      "default": "0",
      "comment": "total MSS file count"
    },
    {
      "name": "webcnt",
      "type": "int",
      "default": "0",
      "comment": "all web data file count"
    }
  ],
  "comment": "HISTORICAL DATASET INFORMATION"
}