{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "10/27/2005",
    "description": "table of order data usage information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "ousage",
  "pkey": "oidx",
  "column": [
    {
      "name": "oidx",
      "type": "serial",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "auto generated ousage index, internal use"
    },
    {
      "name": "wuid_request",
      "type": "int",
      "comment": "reference to wuser.wuid, 0 if missing email"
    },
    {
      "name": "dss_uname",
      "type": "varchar(20)",
      "comment": "DSS staff user login name"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid"
    },
    {
      "name": "date_request",
      "type": "date",
      "comment": "date dynamic data requested"
    },
    {
      "name": "date_closed",
      "type": "date",
      "comment": "date the order closed"
    },
    {
      "name": "quarter",
      "type": "smallint",
      "comment": "quarter of the year data accessed"
    },
    {
      "name": "size_request",
      "type": "bigint",
      "comment": "bytes of data requested"
    },
    {
      "name": "size_input",
      "type": "bigint",
      "comment": "bytes of data processed as input"
    },
    {
      "name": "pay_method",
      "type": "varchar(20)",
      "comment": "payment methods: nochg,invce, and etc."
    },
    {
      "name": "amount",
      "type": "int",
      "comment": "payment in dollars"
    },
    {
      "name": "project",
      "type": "varchar(10)",
      "comment": "project name, normally x if unknown"
    },
    {
      "name": "method",
      "type": "varchar(20)",
      "comment": "delv. methods: Ftp,Tape,Cd,Disk,Paper,cArt,Micro"
    },
    {
      "name": "count",
      "type": "int",
      "comment": "count of medium used for delivering data"
    },
    {
      "name": "order_number",
      "type": "varchar(30)",
      "default": "'0'",
      "comment": "order number assigned"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "IP address"
    }
  ],
  "index": [
    {
      "name": "ousage_idx_1",
      "column": "wuid_request"
    },
    {
      "name": "ousage_idx_2",
      "column": "dsid"
    },
    {
      "name": "ousage_idx_3",
      "column": "date_request"
    },
    {
      "name": "ousage_idx_4",
      "column": "order_number"
    },
    {
      "name": "ousage_idx_5",
      "column": "size_request"
    },
    {
      "name": "ousage_idx_6",
      "column": "method"
    }
  ],
  "comment": "ORDER DATA FILE USAGE"
}