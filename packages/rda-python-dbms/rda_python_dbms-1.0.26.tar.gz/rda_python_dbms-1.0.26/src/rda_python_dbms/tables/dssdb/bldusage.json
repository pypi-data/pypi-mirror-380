{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "10/21/2014",
    "description": "table of instantly-built reuqest data usage information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "bldusage",
  "column": [
    {
      "name": "email",
      "type": "varchar(80)",
      "comment": "user email address"
    },
    {
      "name": "org_type",
      "type": "varchar(5)",
      "comment": "organization type like DSS, UCAR or UNIV"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "user country name"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid"
    },
    {
      "name": "gindex",
      "type": "int",
      "default": "0",
      "comment": "if > 0, under a subgroup in a dataset"
    },
    {
      "name": "date",
      "type": "date",
      "comment": "date data built"
    },
    {
      "name": "time",
      "type": "time",
      "default": "'00:00:00'",
      "comment": "time data built"
    },
    {
      "name": "runtime",
      "type": "int",
      "comment": "executing time for build the request"
    },
    {
      "name": "quarter",
      "type": "smallint",
      "comment": "quarter of the year data built"
    },
    {
      "name": "size",
      "type": "bigint",
      "comment": "bytes of data built"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "IP address"
    },
    {
      "name": "engine",
      "type": "varchar(255)",
      "comment": "engine request submitted, i.e. IDV 5.0u1"
    },
    {
      "name": "rinfo",
      "type": "text",
      "comment": "instant request information"
    }
  ],
  "index": [
    {
      "name": "bldusage_idx_1",
      "column": "quarter"
    },
    {
      "name": "bldusage_idx_2",
      "column": "dsid"
    },
    {
      "name": "bldusage_idx_3",
      "column": "date, time, email",
      "unique": "UNIQUE"
    },
    {
      "name": "bldusage_idx_4",
      "column": "email"
    },
    {
      "name": "bldusage_idx_5",
      "column": "engine"
    }
  ],
  "comment": "INSTANT BUILT DATA USAGE"
}