{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "11/09/2007",
    "description": "table for specialist defined option key/value pairs"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "dsokeys",
  "pkey": "dsid, okey",
  "column": [
    {
      "name": "dsid",
      "type": "varchar(9)",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "dataset id number"
    },
    {
      "name": "okey",
      "type": "varchar(40)",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "specialist defined key"
    },
    {
      "name": "value",
      "type": "varchar(128)",
      "comment": "value of the defined key"
    }
  ],
  "comment": "DATASET KEY-VALUE INFORMATION"
}