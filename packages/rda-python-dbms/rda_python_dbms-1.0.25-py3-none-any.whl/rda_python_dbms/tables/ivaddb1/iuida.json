{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "03/23/2010",
    "description": "table of icoads IMMA data for the unique Report ID section"
  },
  "database": "rdadb",
  "schema": "ivaddb",
  "name": "iuida",
  "pkey": "iidx",
  "column": [
    {
      "name": "iidx",
      "type": "bigint",
      "pkey": "PRIMARY KEY",
      "comment": "icoads record id"
    },
    {
      "name": "date",
      "type": "date",
      "notnull": "NOT NULL",
      "comment": "UTC date of the record"
    },
    {
      "name": "uid",
      "type": "char(6)",
      "comment": "Report ID"
    },
    {
      "name": "rn1",
      "type": "char(1)",
      "comment": "release no.: primary"
    },
    {
      "name": "rn2",
      "type": "char(1)",
      "comment": "release no.: secondary"
    },
    {
      "name": "rn3",
      "type": "char(1)",
      "comment": "release no.: tertiary"
    },
    {
      "name": "rsa",
      "type": "smallint",
      "comment": "release status indicator"
    },
    {
      "name": "irf",
      "type": "smallint",
      "comment": "intermediate reject flag"
    }
  ],
  "index": [
    {
      "name": "iuida_idx_1",
      "column": "uid, rn3",
      "unique": "UNIQUE"
    },
    {
      "name": "iuida_idx_2",
      "column": "date"
    }
  ],
  "comment": "Unique Report ID attm"
}
