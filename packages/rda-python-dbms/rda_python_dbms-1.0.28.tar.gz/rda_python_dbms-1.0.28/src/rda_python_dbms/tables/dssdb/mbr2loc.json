{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "06/24/2013",
    "description": "table of UCAR member universities and location information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "mbr2loc",
  "pkey": "midx",
  "column": [
    {
      "name": "midx",
      "type": "smallserial",
      "pkey": "PRIMARY KEY",
      "comment": "auto increment member index"
    },
    {
      "name": "mid",
      "type": "varchar(128)",
      "comment": "Full name of UCAR member university"
    },
    {
      "name": "domain",
      "type": "varchar(50)",
      "comment": "internet domain name"
    },
    {
      "name": "pattern",
      "type": "varchar(255)",
      "comment": "regular expression matching pattern"
    },
    {
      "name": "lat",
      "type": "int",
      "comment": "latitude in 1/1000 degree N"
    },
    {
      "name": "lon",
      "type": "int",
      "comment": "longitude in 1/1000 degree E"
    },
    {
      "name": "city",
      "type": "varchar(50)",
      "comment": "city name"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "country code"
    }
  ],
  "index": [
    {
      "name": "mbr2loc_idx_1",
      "column": "mid",
      "unique": "UNIQUE"
    },
    {
      "name": "mbr2loc_idx_2",
      "column": "domain",
      "unique": "UNIQUE"
    }
  ],
  "comment": "MEMBER 2 LOCATION"
}