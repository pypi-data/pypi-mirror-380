{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "06/24/2013",
    "description": "table of ip addresses and location information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "ip2loc",
  "pkey": "ip",
  "column": [
    {
      "name": "ip",
      "type": "varchar(15)",
      "pkey": "PRIMARY KEY",
      "comment": "ip address"
    },
    {
      "name": "lat",
      "type": "int",
      "comment": "latitude in 1/1000 degree N"
    },
    {
      "name": "lon",
      "type": "int",
      "comment": "longitude in 1/1000 degree E"
    },
    {
      "name": "city",
      "type": "varchar(50)",
      "comment": "city name"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "country code"
    }
  ],
  "comment": "MEMBER 2 LOCATION"
}