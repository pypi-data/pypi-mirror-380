{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "01/28/2005",
    "description": "table of web online data usage information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "wusage",
  "pkey": "widx",
  "column": [
    {
      "name": "widx",
      "type": "serial",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "auto generated wusage index, internal use"
    },
    {
      "name": "wid",
      "type": "int",
      "comment": "reference to wfile.wid"
    },
    {
      "name": "wuid_read",
      "type": "int",
      "comment": "reference to wuser.wuid, 0 if missing email"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid at the time of read"
    },
    {
      "name": "date_read",
      "type": "date",
      "comment": "date file read"
    },
    {
      "name": "time_read",
      "type": "time",
      "comment": "time file read"
    },
    {
      "name": "quarter",
      "type": "smallint",
      "comment": "quarter of the year data accessed"
    },
    {
      "name": "size_read",
      "type": "bigint",
      "comment": "bytes of data read"
    },
    {
      "name": "method",
      "type": "varchar(20)",
      "comment": "download methods: WEB, CURL, MGET, FTP and MGET"
    },
    {
      "name": "locflag",
      "type": "char(1)",
      "default": "'G'",
      "comment": "location flag: Glade or Object"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "IP address"
    }
  ],
  "index": [
    {
      "name": "wusage_idx_1",
      "column": "wuid_read"
    },
    {
      "name": "wusage_idx_2",
      "column": "dsid"
    },
    {
      "name": "wusage_idx_3",
      "column": "wid, date_read, time_read, method",
      "unique": "UNIQUE"
    },
    {
      "name": "wusage_idx_4",
      "column": "date_read"
    },
    {
      "name": "wusage_idx_5",
      "column": "method"
    },
    {
      "name": "wusage_idx_7",
      "column": "size_read"
    },
    {
      "name": "wusage_idx_8",
      "column": "ip"
    }
  ],
  "comment": "WEB ONLINE DATA FILE USAGE"
}