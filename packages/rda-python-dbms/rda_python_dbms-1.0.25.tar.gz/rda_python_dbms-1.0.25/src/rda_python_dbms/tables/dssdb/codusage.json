{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "10/21/2014",
    "description": "table of custom OPeNDAP data usage information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "codusage",
  "pkey": "codidx",
  "column": [
    {
      "name": "codidx",
      "type": "serial",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "auto generated Custom OPeNDAP index"
    },
    {
      "name": "aid",
      "type": "varchar(10)",
      "comment": "aggregation ID"
    },
    {
      "name": "date",
      "type": "date",
      "comment": "date data accessed"
    },
    {
      "name": "time",
      "type": "time",
      "comment": "time first data accessed"
    },
    {
      "name": "period",
      "type": "int",
      "comment": "period in second for data accessed"
    },
    {
      "name": "email",
      "type": "varchar(80)",
      "comment": "user email address"
    },
    {
      "name": "org_type",
      "type": "varchar(5)",
      "comment": "organization type like DSS, UCAR or UNIV"
    },
    {
      "name": "region",
      "type": "varchar(80)",
      "comment": "region name, state or province"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "user country name"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid"
    },
    {
      "name": "quarter",
      "type": "smallint",
      "comment": "quarter of the year data accessed"
    },
    {
      "name": "size",
      "type": "bigint",
      "comment": "bytes of data accessed"
    },
    {
      "name": "count",
      "type": "int",
      "comment": "count of log netries"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "IP address"
    },
    {
      "name": "engine",
      "type": "varchar(255)",
      "comment": "engine data accessed, i.e. IDV 5.0u1"
    }
  ],
  "index": [
    {
      "name": "codusage_idx_1",
      "column": "dsid"
    },
    {
      "name": "codusage_idx_2",
      "column": "aid, date",
      "unique": "UNIQUE"
    },
    {
      "name": "codusage_idx_3",
      "column": "email"
    },
    {
      "name": "codusage_idx_4",
      "column": "quarter"
    }
  ],
  "comment": "OPeNDAP DATA USAGE"
}