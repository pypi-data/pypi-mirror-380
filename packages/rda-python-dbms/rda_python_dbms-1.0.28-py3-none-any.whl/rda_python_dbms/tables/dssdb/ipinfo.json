{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "03/28/2023",
    "description": "table of IP address information, including all IP addresses, from which users get RDA data"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "ipinfo",
  "pkey": "ipidx",
  "column": [
    {
      "name": "ipidx",
      "type": "serial",
      "pkey": "PRIMARY KEY",
      "notnull": "NOT NULL",
      "comment": "auto generated ipinfo index for internal use"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "ip address"
    },
    {
      "name": "hostname",
      "type": "varchar(128)",
      "comment": "host name for the ip"
    },
    {
      "name": "lat",
      "type": "real",
      "comment": "ip location latitude"
    },
    {
      "name": "lon",
      "type": "real",
      "comment": "ip location longitude"
    },
    {
      "name": "stat_flag",
      "type": "char(1)",
      "default": "'M'",
      "comment": "A-active, I-inactive, M-missing info"
    },
    {
      "name": "org_type",
      "type": "varchar(5)",
      "comment": "organization type like DSS, UCAR or UNIV"
    },
    {
      "name": "org_name",
      "type": "varchar(256)",
      "comment": "name of the organization"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "user country name"
    },
    {
      "name": "region",
      "type": "varchar(80)",
      "comment": "region name for the ip, state or province"
    },
    {
      "name": "city",
      "type": "varchar(80)",
      "comment": "city name for the ip"
    },
    {
      "name": "postal",
      "type": "varchar(20)",
      "comment": "ip location postal code"
    },
    {
      "name": "timezone",
      "type": "varchar(128)",
      "comment": "ip location time zone"
    },
    {
      "name": "adddate",
      "type": "date",
      "comment": "date ip address added"
    },
    {
      "name": "ipinfo",
      "type": "json",
      "comment": "original full ipinfo json data"
    }
  ],
  "index": [
    {
      "name": "ipinfo_idx_1",
      "column": "ip",
      "unique": "UNIQUE"
    },
    {
      "name": "ipinfo_idx_2",
      "column": "org_type"
    },
    {
      "name": "ipinfo_idx_3",
      "column": "stat_flag"
    },
    {
      "name": "ipinfo_idx_4",
      "column": "hostname"
    }
  ],
  "comment": "IP ADDRESS INFORMATION"
}