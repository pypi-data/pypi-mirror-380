{
  "header": {
    "author": "Zaihua Ji, zji@ucar.edu",
    "date": "2025-08-12",
    "description": "table of AWS data usage information"
  },
  "database": "rdadb",
  "schema": "dssdb",
  "name": "awsusage",
  "column": [
    {
      "name": "email",
      "type": "varchar(80)",
      "comment": "user email address"
    },
    {
      "name": "org_type",
      "type": "varchar(5)",
      "comment": "organization type like DSS, UCAR or UNIV"
    },
    {
      "name": "country",
      "type": "varchar(50)",
      "comment": "user country name"
    },
    {
      "name": "region",
      "type": "varchar(80)",
      "comment": "region name, state or province"
    },
    {
      "name": "dsid",
      "type": "varchar(9)",
      "comment": "reference to dataset.dsid"
    },
    {
      "name": "date",
      "type": "date",
      "comment": "date data accessed"
    },
    {
      "name": "time",
      "type": "time",
      "default": "'00:00:00'",
      "comment": "time data accessed"
    },
    {
      "name": "quarter",
      "type": "smallint",
      "comment": "quarter of the year data accessed"
    },
    {
      "name": "size",
      "type": "bigint",
      "comment": "bytes of data accessed"
    },
    {
      "name": "fcount",
      "type": "int",
      "default": "0",
      "comment": "count of file accessed"
    },
    {
      "name": "method",
      "type": "varchar(20)",
      "comment": "access method like opendap, ncss, cdmremote, wcs or wms"
    },
    {
      "name": "ip",
      "type": "varchar(39)",
      "comment": "IP address"
    },
    {
      "name": "engine",
      "type": "varchar(512)",
      "comment": "engine data accessed, i.e. IDV 5.0u1"
    }
  ],
  "index": [
    {
      "name": "awsusage_idx_1",
      "column": "quarter"
    },
    {
      "name": "awsusage_idx_2",
      "column": "dsid"
    },
    {
      "name": "awsusage_idx_3",
      "column": "date, time, ip",
      "unique": "UNIQUE"
    },
    {
      "name": "awsusage_idx_4",
      "column": "email"
    },
    {
      "name": "awsusage_idx_5",
      "column": "engine"
    },
    {
      "name": "awsusage_idx_6",
      "column": "method"
    }
  ],
  "comment": "THREDDS DATA USAGE"
}