apiVersion: 1

datasources:
  - name: InfluxDB InfluxQL
    type: influxdb
    url: {{endpoint}}
    access: proxy
    editable: true
    isDefault: true
    secureJsonData:
      httpHeaderValue1: 'Token {{token}}'
    jsonData:
      dbName: {{database}}
      httpHeaderName1: 'Authorization'
      timeInterval: 10s
  - name: 'xronos-traces'
    type: 'xronos-xronostraces-datasource'
    access: proxy
    uid: 'xronos-xronostraces-datasource'
    url: {{endpoint}}
    database: {{database}}
    isDefault: false
    orgId: 1
    version: 1
    editable: true
    jsonData:
      organization: "{{organization}}"
    secureJsonData:
      token: "{{token}}"
  - name: InfluxDB Flux
    type: influxdb
    url: {{endpoint}}
    access: proxy
    editable: true
    isDefault: false
    secureJsonData:
      token: "{{token}}"
    jsonData:
      version: Flux
      organization: "{{organization}}"
      defaultBucket: {{database}}
      timeInterval: 10s
