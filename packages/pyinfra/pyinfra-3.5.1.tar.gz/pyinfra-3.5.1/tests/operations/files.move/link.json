{
    "require_platform": ["Darwin", "Linux"],
    "args": ["tmp/testfile", "tmp2"],
    "facts": {
        "files.File": {
            "path=tmp/testfile": false,
            "path=tmp2/testfile": null
        },
        "files.Link": {
            "path=tmp/testfile": {
                "mode": 600
            }
        },
        "files.Directory": {
            "path=tmp2": {
                "mode": 700
            }
        }
    },
    "commands": ["mv tmp/testfile tmp2"]
}
