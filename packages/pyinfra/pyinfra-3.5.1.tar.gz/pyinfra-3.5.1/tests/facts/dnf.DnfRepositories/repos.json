{
    "command": "(! test -f /etc/dnf.conf || cat /etc/dnf.conf) && (cat /etc/dnf.repos.d/*.repo || true) && (cat /etc/yum.repos.d/*.repo || true)",
    "requires_command": "dnf",
    "output": [
        "",
        "[somerepo]",
        "baseurl=abc",
        "",
        "[anotherrepo]"
    ],
    "fact": [
        {
            "repoid": "somerepo",
            "name": "somerepo",
            "baseurl": "abc"
        },
        {
            "repoid": "anotherrepo",
            "name": "anotherrepo"
        }
    ]
}
