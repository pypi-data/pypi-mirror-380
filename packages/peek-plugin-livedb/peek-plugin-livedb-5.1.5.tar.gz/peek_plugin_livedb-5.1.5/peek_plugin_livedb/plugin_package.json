{
    "plugin": {
        "title": "Live DB",
        "packageName": "peek_plugin_livedb",
        "version": "5.1.5",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "field",
        "office",
        "logic",
        "storage",
        "admin",
        "agent",
        "worker",
        "doc-dev",
        "admin-doc"
    ],
    "admin-doc": {
        "docDir": "admin-doc",
        "docRst": "index.rst",
        "hasApi": true
    },
    "logic": {},
    "storage": {
        "alembicDir": "_private/alembic"
    },
    "admin": {
        "showHomeLink": true,
        "appDir": "_private/admin-app",
        "appModule": "livedb.module#LiveDBModule",
        "moduleDir": "plugin-module"
    },
    "agent": {},
    "worker": {},
    "field" : {
        "subprocessGroup": "livedb+diagram"
    },
    "office" : {
        "subprocessGroup": "livedb+diagram"
    }
}
