          Subroutine DMCompositeGetEntries4(dm1, d1, d2, d3, d4, ierr)
            use, intrinsic :: ISO_C_binding
            import tDM
            DM dm1
            PetscInt d2, d4
            DM d1, d3
            PetscErrorCode ierr
          End Subroutine
        End Interface

