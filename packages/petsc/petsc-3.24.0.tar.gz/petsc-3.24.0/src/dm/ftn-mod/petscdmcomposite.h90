            import tDM
            DM dm1
            DM d1
            PetscErrorCode ierr
          End Subroutine

          Subroutine DMCompositeGetEntries2(dm1, d1, d2, ierr)
            use, intrinsic :: ISO_C_binding
            import tDM
            DM dm1
