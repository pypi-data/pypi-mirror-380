/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "strict": false,
    "paths": {
      "@peek/*": [
        "src/@peek/*"
      ],
      "@_peek/*": [
        "src/@_peek/*"
      ],
      "@angular/*": [
        "node_modules/@angular/*"
      ]
  }
  },
  "files": [
    "src/main.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
