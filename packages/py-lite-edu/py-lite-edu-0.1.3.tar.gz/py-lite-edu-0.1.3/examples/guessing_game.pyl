    while attempts < max_attempts:
        attempts = attempts + 1
        print("–ü–æ–ø—ã—Ç–∫–∞", attempts, "–∏–∑", max_attempts)
        
        # –í —Ä–µ–∞–ª—å–Ω–æ–π –∏–≥—Ä–µ –∑–¥–µ—Å—å –±—ã–ª –±—ã input()
