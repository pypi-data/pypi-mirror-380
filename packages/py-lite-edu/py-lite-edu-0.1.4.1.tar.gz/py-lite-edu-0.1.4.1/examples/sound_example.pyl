# Пример использования модуля sound
import sound
import time

# Установка громкости
sound.set_volume(0.8)

# Попытка воспроизвести звук
print("Воспроизвожу звук 'click'...")
sound.play("click") 

# Ждем 1 секунду, чтобы звук успел воспроизвестись
time.sleep(1)

print("Звук 'click' должен был воспроизвестись.")

