<?xml version="1.0"?>
<odoo>
    <record id="view_multimedia_tree_lead_line_infos" model="ir.ui.view">
        <field name="name">crm.lead.line multimedia infos tree view (in crm lead)</field>
        <field name="model">crm.lead.line</field>
        <field name="arch" type="xml">
        <tree editable="bottom">
            <field name="active" invisible="1"/>
            <field name="id" attrs="{'readonly':True}"/>
            <field name="product_id" attrs="{'readonly':True}"/>
            <field name="create_date"/>
            <field name="iban"/>
            <field name="create_reason"/>
            <field name="tree_view_notes"/>
        </tree>
        </field>
    </record>
</odoo>
