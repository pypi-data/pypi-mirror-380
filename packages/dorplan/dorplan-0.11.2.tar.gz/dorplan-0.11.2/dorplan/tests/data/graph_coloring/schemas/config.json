{
    "$schema": "http://json-schema.org/schema#",
    "type": "object",
    "properties": {
        "timeLimit": {"type": "number"},
        "report": {
            "type": "object",
            "properties": {
                "name": {"type":  "string"}
            }
        },
        "solver": {
            "type": "string",
            "enum": ["ortools", "pulp", "timefold", "networkx"],
            "default": "ortools"
        }
    }
}
