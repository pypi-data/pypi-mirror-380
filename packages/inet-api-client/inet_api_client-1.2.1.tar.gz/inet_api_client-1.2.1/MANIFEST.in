# 包含必要的文件到分发包中
include README.md
include LICENSE
include MANIFEST.in
include pyproject.toml

# 包含包目录中的所有文件
recursive-include sky_api_client *.py

# 排除不必要的文件
global-exclude *.pyc
global-exclude *.pyo
global-exclude *.pyd
global-exclude __pycache__
global-exclude .git*
global-exclude .DS_Store
global-exclude *.so
