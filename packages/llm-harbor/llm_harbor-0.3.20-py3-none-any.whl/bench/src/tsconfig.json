{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["ESNext"],
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "allowImportingTsExtensions": true
  },
  "include": ["**/*.ts"]
}